#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'my-robot-controller','console_scripts','test_node'
__requires__ = 'my-robot-controller'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('my-robot-controller', 'console_scripts', 'test_node')()
    )
